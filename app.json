{
  "expo": {
    "name": "expo-three-example",
    "slug": "expo-three-example",
    "scheme": "expo-three-example",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "dev.expo.expothree.example",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.expo.expothree.example",
      "permissions": ["INTERNET"],
      "softwareKeyboardLayoutMode": "adjustResize"
    },
    "web": {
      "splash": {
        "backgroundColor": "#FFFFFF"
      },
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "assets": ["./assets/images/"]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  }
}
